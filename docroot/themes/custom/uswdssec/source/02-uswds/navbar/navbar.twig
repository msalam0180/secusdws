{% set classes = [
  'usa-navbar',
  modifier_classes ? modifier_classes : '',
]|join(' ')|trim %}

<div {{ add_attributes({ 'class': classes }) }}>
  {% block content %}
    {% include "@uswds/logo/logo.twig" with {
      'site_name': site_name,
      'url': '/',
      'site_logo': site_logo,
      'attributes': '',
      'is_image': true
    } %}   
  {% endblock %}
  <button class="usa-menu-btn">{{ 'Menu'|t }}</button>
</div>
