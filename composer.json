  "require-dev": {
    "behat/mink": "^1.7",
    "phpunit/phpunit": "^9.5"
  },
  "conflict": {
    "drupal/core": "7.*"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "drupal-scaffold": {
      "file-mapping": {
        "[web-root]/robots.txt": {
          "append": "custom/robots.txt"
        },
        "[web-root]/.eslintignore":{
          "append": "custom/.eslintignore"
        },
        "[web-root]/.eslintrc.json":{
          "mode": "skip"
        },
        "[web-root]/.htaccess":{
          "mode": "skip"
        },
        "[web-root]/sites/development.services.yml": {
          "mode": "skip"
        }
      },
      "locations": {
        "web-root": "./docroot"
      }
    },
    "installer-paths": {
      "docroot/sites/investorgov/modules/contrib/{$name}": [
        "drupal/menu_block",
        "drupal/menu_block_title",
        "drupal/path_redirect_import",
        "drupal/search_api_autocomplete",
        "drupal/search_api_spellcheck",
        "drupal/viewfield"
      ],
      "docroot/core": ["type:drupal-core"],
      "docroot/modules/contrib/{$name}": ["type:drupal-module"],
      "docroot/profiles/contrib/{$name}": ["type:drupal-profile"],
      "docroot/themes/contrib/{$name}": ["type:drupal-theme"],
      "docroot/libraries/{$name}": ["type:drupal-library"]
    },
    "drupal-libraries": {
      "datatables": "https://datatables.net/releases/DataTables-1.9.3.zip",
      "fakeobjects": "https://download.ckeditor.com/fakeobjects/releases/fakeobjects_4.18.0.zip",
      "flexslider": "https://github.com/woocommerce/FlexSlider/archive/refs/tags/2.6.3.zip",
      "swiftmailer": "https://github.com/swiftmailer/swiftmailer/archive/refs/tags/v5.4.8.zip"
    },
    "enable-patching": true,
    "patches": {
      "ckeditor-plugin/link": {
        "Drupal Issue: https://www.drupal.org/project/anchor_link/issues/2802085": "docroot/modules/contrib/patches/custom-removing-duplicate-edit-link-wysiwyg-based-on-2802085-18.patch"
      },
      "drupal/access_unpublished": {
        "Custom patch": "docroot/modules/contrib/patches/access_unpublished-entity_type_does_not_exist.patch",
        "Custom patch - Remove unlimited option": "docroot/modules/contrib/patches/osss-15874-access-unpublished-remove-unlimited.patch"
      },
      "drupal/adminimal_admin_toolbar":{
        "D9 icon update patch https://www.drupal.org/project/adminimal_admin_toolbar/issues/3163092": "docroot/modules/contrib/patches/icon-update-3163092-4.patch"
      },
      "drupal/cacheexclude": {
        "Fix non-existent service path.alias_manager https://www.drupal.org/project/cacheexclude/issues/3201844": "docroot/modules/contrib/patches/cacheexclude-3201844-path_alias_manager-8.patch"
      },
      "drupal/core": {
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/3039185" : "docroot/modules/contrib/patches/3039185-46.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2918537": "docroot/modules/contrib/patches/core-book-outline-revisions-validation-2918537-61.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2314185": "docroot/modules/contrib/patches/drupalbehaviorerror-2314185-15.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2985199": "docroot/modules/contrib/patches/specify_sitepath_cache_rebuild_extension_discovery-2985199-3.patch",
        "Drupal Issue: https://www.drupal.org/project/drupal/issues/2959134": "docroot/modules/contrib/patches/cached-rss-feeds-http-header-2959134-14.patch",
        "Drupal issue: https://www.drupal.org/project/drupal/issues/3209129": "docroot/modules/contrib/patches/3209129-5.patch"
      },
      "drupal/http_response_headers": {
        "Drupal issue: https://www.drupal.org/project/http_response_headers/issues/3193303": "docroot/modules/contrib/patches/http_response_headers-3193303.patch",
        "Drupal issue: https://www.drupal.org/project/http_response_headers/issues/3193304": "docroot/modules/contrib/patches/http_response_headers-3193304.patch",
        "Drupal issue: https://www.drupal.org/project/http_response_headers/issues/3316677": "docroot/modules/contrib/patches/3316677-4.patch"
      },
      "drupal/inline_entity_form": {
        "Allow to hide the Edit button in Complex widget": "docroot/modules/contrib/patches/optional_edit-3143422-2.patch"
      },
      "drupal/media_entity_file_replace": {
        "Clear image cache after updating media image: https://www.drupal.org/project/media_entity_file_replace/issues/3106363": "docroot/modules/contrib/patches/media_entity_file_replace-clear_image_cache-3106363-4.patch"
      },
      "drupal/paragraphs": {
        "Drupal Issue: https://www.drupal.org/project/paragraphs/issues/2901390": "docroot/modules/contrib/patches/paragraphs-set_langcode_widgets-2901390-29.patch"
      },
      "drupal/scheduled_updates": {
        "D9 Compatibility Fix patch https://www.drupal.org/project/scheduled_updates/issues/3172330": "docroot/modules/contrib/patches/scheduled_updates-3172330-24.patch"
      },
      "drupal/scheduler": {
        "Drupal issue: https://www.drupal.org/project/scheduler/issues/3224340": "docroot/modules/contrib/patches/3224340-22.local_tasks_alter.patch"
      },
      "drupal/symfony_mailer": {
        "Custom patch: fix attachment filepath error": "docroot/modules/contrib/patches/OSSS-20705-fixed-attachment-filepath-error.patch"
      },
      "drupal/views_ifempty": {
        "D9 Compatibility Rector Fixes https://www.drupal.org/project/views_ifempty/issues/3142112": "docroot/modules/contrib/patches/views_ifempty.1.x-dev.rector.patch"
      },
      "drupal/webform": {
        "Custom patch: Hiding option to include file with webform export result": "docroot/modules/contrib/patches/osss-13961-hide-option-to-download-files.patch"
      }
    },
    "composer-exit-on-patch-failure": true
  },
  "repositories": [
    {
      "type": "package",
      "package": [
        {
          "name": "ckeditor-plugin/link",
          "type": "drupal-library",
          "version": "4.18.0",
          "dist": {
            "type": "zip",
            "url": "https://download.ckeditor.com/link/releases/link_4.18.0.zip"
          }
        }
      ]
    },
    {
      "type": "composer",
      "url": "https://packages.drupal.org/8"
    }
  ],
  "config": {
    "sort-packages": true,
    "discard-changes": true,
    "platform": {
      "php": "8.0"
    },
    "allow-plugins": {
      "composer/installers": true,
      "simplesamlphp/composer-module-installer": true,
      "cweagans/composer-patches": true,
      "drupal/core-composer-scaffold": true,
      "drupal/core-project-message": true,
      "zodiacmedia/drupal-libraries-installer": true
    }
  }
}
